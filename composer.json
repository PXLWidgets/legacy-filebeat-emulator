{
    "name": "pxlwidgets/legacy-filebeat-emulator",
    "description": "An emulator for PHP-based filebeat-like log processing",
    "homepage": "https://github.com/PXLWidgets",
    "require": {
        "php": ">=5.6.0",
        "ext-json": "*"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "PXLWidgets\\FilebeatEmulator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PXLWidgets\\FilebeatEmulator\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "platform": {
            "php": "5.6.0"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
